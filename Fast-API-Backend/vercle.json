{
    "version": 2,
    "builds": [
      {
        "src": "scores/Scores_prediction_api.py",
        "use": "@vercel/python"
      },
      {
        "src": "genres_styles/genre_style_prediction_api.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/scores/(.*)",
        "dest": "/scores/Scores_prediction_api.py"
      },
      {
        "src": "/genres_styles/(.*)",
        "dest": "/genres_styles/genre_style_prediction_api.py"
      }
    ]
  }
  